<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myui" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Myapplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:uiOptions="splitActionBarWhenNarrow" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ImageAn"
            android:label="单张图片手势缩放" />
        <activity
            android:name=".activity.ImageGroup"
            android:label="多张滑动图片放大" />
        <activity
            android:name=".activity.ImageCarousel"
            android:label="图片轮播" />
        <activity
            android:name=".activity.ImageShape"
            android:label="各种imageview形状" />
        <activity
            android:name=".activity.DeleteListViewMainActivity"
            android:label="仿QQ侧滑listview" />
        <activity
            android:name=".activity.DragViewActivity"
            android:label="随意拖动view" />
        <activity
            android:name=".activity.SlideButtonActivity"
            android:label="@string/title_activity_slide_button" >
        </activity>
        <activity
            android:name=".activity.SuspendScrollViewActivity"
            android:label="@string/title_activity_suspend_scroll_view" >
        </activity>
        <activity
            android:name=".activity.RecyclerViewActivity"
            android:label="@string/title_activity_recycler_view" >
        </activity>
        <activity
            android:name=".activity.CardViewActivity"
            android:label="@string/title_activity_card_view" >
        </activity>
        <activity
            android:name=".activity.FragmentCutPagerTitleStripActivity"
            android:label="@string/title_activity_fragment_cut_pager_title_strip" >
        </activity>
        <activity
            android:name=".activity.FragmentCutPagerSlidingTabStripActivity"
            android:label="@string/title_activity_fragment_cut_pager_sliding_tab_strip" >
        </activity>
    </application>

</manifest>
